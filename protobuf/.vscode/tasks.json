{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "gcc.exe",
      "args": [
        "-g",
        "-o",
        "${workspaceFolder}/main.exe",
        "${workspaceFolder}/main.cpp",
        "${workspaceFolder}/test.pb.cc",
        "-IE:/CProgram/protobuf-32.0/_deps/absl-src",
        "-IE:/CProgram/protobuf-32.0/src",
        "-L${workspaceFolder}",
        "-LC:\\Program Files (x86)\\protobuf\\lib",
        "-labsl_log_internal_check_op",
        "-lprotobuf",
        "-lstdc++",
        "-lpthread"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$msCompile"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
